{
    "name": "alisaleem/wordle-cli",
    "description": "Wordle CLI",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ali Saleem",
            "email": "alisaleem@outlook.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/framework": "^8.8",
        "nunomaduro/termwind": "^1.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["wordle"]
}
